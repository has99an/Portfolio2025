{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.{js,css,json,ico,png,svg,jpg,jpeg,gif,webp,woff,woff2,ttf,eot,webmanifest,jsx,mjs}",
      "/favicon.png",
      "/logo.png",
      "/vite.svg",
      "/models/*",
      "/src/*"
    ]
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/src/*",
      "allowedRoles": ["anonymous"],
      "statusCode": 404
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".jsx": "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css",
    ".json": "application/json"
  }
}


