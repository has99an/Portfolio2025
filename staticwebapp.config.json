{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*.{js,css,json,ico,png,svg,jpg,jpeg,gif,webp,woff,woff2,ttf,eot,webmanifest,jsx}"]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".jsx": "application/javascript",
    ".mjs": "application/javascript"
  }
}


